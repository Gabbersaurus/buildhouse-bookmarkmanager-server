    "devDependencies": {
        "nodemon": "^2.0.7"
    },
    "dependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/node": "^14.14.34",
        "@typescript-eslint/eslint-plugin": "^4.17.0",
        "@typescript-eslint/parser": "^4.17.0",
        "apollo-server": "^2.21.1",
        "base64url": "^3.0.1",
        "bcrypt": "^5.0.1",
